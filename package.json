{
  "name": "@triniti/admin-ui-plugin",
  "version": "1.0.0",
  "description": "The admin-ui is a javaScript plugin for Triniti apps.",
  "repository": {
    "type": "git",
    "url": "https://github.com/triniti/admin-ui-plugin-js.git"
  },
  "license": "Apache-2.0",
  "module": "./index.js",
  "jsnext:main": "./index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run build:babel && npm run build:prepub && npm run build:rm-scripts",
    "build:babel": "BABEL_ENV=es6 babel src --out-dir ./dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./dist && cpx 'src/**/*.{scss,sass,svg}' ./dist",
    "build:rm-scripts": "jq 'del(.babel)' ./package.json | jq 'del(.scripts)' > ./dist/package.json",
    "clean": "rimraf ./dist",
    "lint": "BABEL_ENV=cjs eslint src",
    "pub": "npm run build && cd dist && npm publish --access public",
    "test": "BABEL_ENV=cjs tape -r ./src/bootstrap.test.js './src/**/*.test.js*' | faucet"
  },
  "peerDependencies": {
    "@gdbots/common": "~0.1 || ^1.0.0",
