{
  "name": "@triniti/admin-ui-plugin",
  "version": "0.5.10",
  "description": "The admin-ui is a javaScript plugin for Triniti apps.",
  "repository": {
    "type": "git",
    "url": "https://github.com/triniti/admin-ui-plugin-js.git"
  },
  "license": "Apache-2.0",
  "module": "./index.js",
  "jsnext:main": "./index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run build:babel && npm run build:prepub && npm run build:rm-scripts",
    "build:babel": "BABEL_ENV=es6 babel src --out-dir ./dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./dist && cpx 'src/**/*.{scss,sass,svg}' ./dist",
    "build:rm-scripts": "jq 'del(.babel)' ./package.json | jq 'del(.scripts)' > ./dist/package.json",
    "clean": "rimraf ./dist",
    "lint": "BABEL_ENV=cjs eslint src",
    "pub": "npm run build && cd dist && npm publish --access public",
    "test": "BABEL_ENV=cjs tape -r ./src/bootstrap.test.js './src/**/*.test.js*' | faucet"
  },
  "peerDependencies": {
    "@gdbots/common": "~0.1 || ^1.0.0",
    "@triniti/app": "~0.4 || ^1.0.0",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "react-universal-component": "^3.0.0",
    "redux": "^4.0.0",
    "sweetalert2": "^7.26.29"
  },
  "dependencies": {
    "react-datepicker": "^1.7.0",
    "react-numeric-input": "^2.2.3",
    "react-select": "^1.3.0",
    "react-stepper-horizontal": "^1.0.11",
    "reactstrap": "^6.5.0"
  },
  "devDependencies": {
    "@gdbots/common": "^0.1.1",
    "@triniti/app": "^0.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
