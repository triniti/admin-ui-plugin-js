    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.2",
    "react-universal-component": "^4.0.0",
    "redux": "^4.0.4",
    "sweetalert2": "^8.16.3"
  },
