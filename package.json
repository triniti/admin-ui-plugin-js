    "react-numeric-input": "^2.2.3",
    "react-select": "^3.0.4",
    "react-stepper-horizontal": "^1.0.11",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
